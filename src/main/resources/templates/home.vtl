<h1>Parcel Calculator</h2>
<h3>Enter your info</h3>

<form action='/parcel'>
  <div class="form-group">
    <label for="height">Height in Inches</label>
    <input id="height" name="height" class="form-control" type="text">
  </div>

  <div class="form-group">
    <label for="width">Width in Inches</label>
    <input id="width" name="width" class="form-control" type="text">
  </div>

  <div class="form-group">
    <label for="length">Length in Inches</label>
    <input id="length" name="length" class="form-control" type="text">
  </div>

  <div class="form-group">
    <label for="weight">Weight in Ounces</label>
    <input id="weight" name="weight" class="form-control" type="text">
  </div>

  <div class="form-group">
    <label for="distance">Miles from Portland</label>
    <input id="distance" name="distance" class="form-control" type="text">
  </div>

  <div class="form-group">
    <label for="speed">Choose Your Shipping Speed</label>
    <select name="speed" id="speed">
      <option value="Ground" id="ground">Ground</option>
      <option value="Express" id="express">Express + $5</option>
      <option value="Overnight" id="overnight">Overnight + $10</option>
    </select>
  </div>

  <div class="form-group">
    <label for="giftwrap">Gift Wrap</label>
    <select name="giftwrap" id="giftwrap">
      <option value="yes" id="yes">Yes + $1 per square foot</option>
      <option value="no" id="no">No</option>
    </select>
  </div>

  <button type="submit" class="btn btn-success">Submit</button>
</form>
